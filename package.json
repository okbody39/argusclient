{
  "name": "SeedClient",
  "version": "0.1.0",
  "description": "SeedVDI, SeedCloud",
  "license": "NOLICENSE",
  "private": true,
  "author": {
    "name": "Peter Han",
    "email": "yesjyhan@gmail.com"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "main": "main.js",
  "scripts": {
    "start": "npm run env:dev -- webpack-dev-server",
    "build": "npm run env:prod -- webpack",
    "prod": "npm run build && electron --noDevServer .",
    "package": "npm run build",
    "package:all": "npm run build",
    "test": "jest -u",
    "lint": "eslint ./src",
    "docs": "jsdoc -r ./src -d docs --verbose",
    "env:dev": "cross-env NODE_ENV=development",
    "env:prod": "cross-env NODE_ENV=production",
    "build:clean": "rimraf -rf dist",
    "build:copy": "copyfiles -u 1 public/* public/**/* dist -e public/index.html",
    "prebuild": "npm run build:clean && npm run build:copy",
    "prepackage:all": "rimraf -rf release",
    "postpackage": "electron-builder build",
    "postpackage:all": "electron-builder build -mwl"
  },
  "dependencies": {
    "antd": "^3.26.7",
    "classnames": "^2.2.6",
    "electron-is-dev": "^1.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-feather": "^2.0.3",
    "react-router-dom": "^5.1.2",
    "styled-components": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@hot-loader/react-dom": "^16.11.0",
    "antd-scss-theme-plugin": "^1.0.8",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-root-import": "^6.4.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babili-webpack-plugin": "^0.1.2",
    "brotli-webpack-plugin": "^1.1.0",
    "copyfiles": "^2.2.0",
    "cross-env": "^5.2.1",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^2.1.1",
    "electron": "^7.1.9",
    "electron-builder": "^22.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-react": "^7.18.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "jest-cli": "^25.1.0",
    "jsdoc": "^3.6.3",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-pxtorem": "^4.0.1",
    "react-hot-loader": "^4.12.18",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "thread-loader": "^2.1.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  },
  "build": {
    "productName": "SeedClient",
    "appId": "net.dfocus.seedclient",
    "files": [
      "dist",
      "main.js",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/@test/config/setupTests.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "babel-jest",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/@test/config/fileMock.js",
      "^components$": "<rootDir>/@test/config/componentsMock.js"
    },
    "moduleDirectories": [
      "src",
      "@test/src",
      "node_modules"
    ]
  }
}
