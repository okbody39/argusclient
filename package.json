{
  "name": "SeedClient",
  "version": "0.1.18",
  "description": "SeedVDI, SeedCloud",
  "license": "UNLICENSE",
  "private": true,
  "author": {
    "name": "Peter Han",
    "email": "yesjyhan@gmail.com"
  },
  "main": "main.js",
  "scripts": {
    "dev": "npm run env:dev -- webpack-dev-server",
    "build": "npm run env:prod -- webpack",
    "start": "npm run build && electron --noDevServer .",
    "package": "npm run build",
    "package:all": "npm run build",
    "test": "jest -u",
    "lint": "eslint ./src",
    "docs": "jsdoc -r ./src -d docs --verbose",
    "env:dev": "cross-env NODE_ENV=development",
    "env:prod": "cross-env NODE_ENV=production",
    "build:clean": "rimraf -rf dist",
    "build:copy": "copyfiles -u 1 public/* public/**/* dist -e public/index.html",
    "prebuild": "npm run build:clean && npm run build:copy",
    "prepackage:all": "rimraf -rf dist",
    "postpackage:mac": "electron-builder build",
    "postpackage:pack": "electron-builder --win --x64 --dir",
    "postpackage:win": "electron-builder build --win --x64",
    "postpackage:linux": "electron-builder build --linux --x64",
    "postpackage:all": "electron-builder build -mwl",
    "build:all": "npm run build && npm run prepackage:all &&  npm run postpackage:all",
    "build:mac": "npm run prepackage:all &&  npm run postpackage:mac",
    "build:win": "npm run prepackage:all &&  npm run postpackage:win",
    "build:linux": "npm run prepackage:all &&  npm run postpackage:linux",
    "publish:linux": "publish ./publisher.json linux-x64-prod",
    "publish:mac": "publish ./publisher.json darwin-x64-prod",
    "publish:win": "publish -d ./publisher.json win32-x64-prod"
  },
  "dependencies": {
    "antd": "^3.26.12",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "electron-builder-squirrel-windows": "^22.4.0",
    "electron-is-dev": "^1.1.0",
    "electron-log": "^4.0.7",
    "electron-simple-updater": "^2.0.9",
    "electron-store": "^5.1.1",
    "event-emitter": "^0.3.5",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "prop-types": "^15.7.2",
    "ps-list": "^7.0.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-feather": "^2.0.3",
    "react-highlight-words": "^0.16.0",
    "react-keyed-file-browser": "^1.7.0",
    "react-moment": "^0.9.7",
    "react-resize-layout": "^0.2.3",
    "react-router-dom": "^5.1.2",
    "react-sparklines": "^1.7.0",
    "regedit": "^3.0.3",
    "styled-components": "^5.0.1",
    "ws": "^7.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@hot-loader/react-dom": "^16.11.0",
    "antd-scss-theme-plugin": "^1.0.8",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-root-import": "^6.4.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babili-webpack-plugin": "^0.1.2",
    "brotli-webpack-plugin": "^1.1.0",
    "copyfiles": "^2.2.0",
    "cross-env": "^7.0.2",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^2.1.1",
    "electron": "^8.1.0",
    "electron-builder": "^22.4.0",
    "electron-simple-publisher": "^2.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-react": "^7.18.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "jest-cli": "^25.1.0",
    "jsdoc": "^3.6.3",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-pxtorem": "^4.0.1",
    "react-hot-loader": "^4.12.18",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "thread-loader": "^2.1.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "updater": {
    "url": "http://cielcloud.iptime.org:8011/updates/{platform}-{arch}-{channel}.json"
  },
  "build": {
    "productName": "SeedClient",
    "appId": "net.dfocus.seedclient",
    "asar": false,
    "files": [
      "dist",
      "app",
      "main.js",
      "preload.js",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.utilities",
      "target": [
        "zip",
        "dmg"
      ],
      "type": "distribution",
      "extendInfo": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      },
      "icon": "./resources/icons/seedclient_icon.icns"
    },
    "dmg": {
      "title": "SeedClient",
      "icon": "./resources/icons/seedclient_icon.icns"
    },
    "win": {
      "target": [
        "squirrel"
      ],
      "icon": "./resources/icons/seedclient_icon.ico",
      "certificateFile": "./resources/www.dfocus.net.pfx",
      "certificatePassword": "dfocus123!"
    },
    "squirrelWindows": {
      "iconUrl": "http://211.232.94.233:1338/seed.ico"
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "zip",
        "AppImage"
      ],
      "category": "Utility",
      "icon": "./resources/icons/256x256.png"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": false,
      "installerLanguages": [
        "en_US",
        "ko_KR"
      ],
      "language": "1042"
    },
    "directories": {
      "buildResources": "resources",
      "output": "dist/"
    }
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/@test/config/setupTests.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "babel-jest",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/@test/config/fileMock.js",
      "^components$": "<rootDir>/@test/config/componentsMock.js"
    },
    "moduleDirectories": [
      "src",
      "@test/src",
      "node_modules"
    ]
  }
}
